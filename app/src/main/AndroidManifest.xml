<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguideapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LearnActivity"
            android:label="@string/learn_text"/>
        <activity android:name=".ShopActivity"
            android:label="@string/shop_text"/>
        <activity android:name=".GetAroundActivity"
            android:label="@string/get_around_text"/>
        <activity android:name=".FunActivity"
            android:label="@string/fun_text">
        </activity>
    </application>

</manifest>

